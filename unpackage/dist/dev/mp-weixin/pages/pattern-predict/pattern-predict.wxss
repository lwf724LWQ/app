
.pattern-predict-container.data-v-fe65a6f0 {
  min-height: 100vh;
  background-color: #f5f5f5;
}
.nav-bar.data-v-fe65a6f0 {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #eee;
}
.nav-left.data-v-fe65a6f0 {
  width: 60rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav-title.data-v-fe65a6f0 {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}
.nav-right.data-v-fe65a6f0 {
  width: 60rpx;
}
.main-content.data-v-fe65a6f0 {
  padding: 30rpx;
}
.scheme-section.data-v-fe65a6f0 {
  margin-bottom: 40rpx;
}
.section-title.data-v-fe65a6f0 {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 20rpx;
}
.scheme-input.data-v-fe65a6f0 {
  background-color: #fff;
  border-radius: 16rpx;
  padding: 30rpx;
  border: 2rpx solid #eee;
}
.issue-info.data-v-fe65a6f0 {
  margin-bottom: 20rpx;
}
.issue-number.data-v-fe65a6f0 {
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}
.scheme-placeholder.data-v-fe65a6f0 {
  padding: 20rpx 0;
}
.placeholder-text.data-v-fe65a6f0 {
  font-size: 28rpx;
  color: #999;
}
.scheme-display.data-v-fe65a6f0 {
  padding: 20rpx 0;
}
.scheme-content.data-v-fe65a6f0 {
  background-color: #f8f8f8;
  padding: 30rpx;
  border-radius: 15rpx;
  margin-top: 20rpx;
}
.scheme-item.data-v-fe65a6f0 {
  margin-bottom: 15rpx;
}
.scheme-name.data-v-fe65a6f0 {
  font-size: 28rpx;
  color: #ff4757;
  font-weight: bold;
  display: block;
  margin-bottom: 10rpx;
}
.scheme-details.data-v-fe65a6f0 {
  display: flex;
  flex-direction: column;
  gap: 8rpx;
}
.scheme-detail.data-v-fe65a6f0 {
  font-size: 26rpx;
  color: #333;
  line-height: 1.5;
}
.image-section.data-v-fe65a6f0 {
  margin-bottom: 40rpx;
}
.image-upload-container.data-v-fe65a6f0 {
  background-color: #fff;
  border-radius: 16rpx;
  padding: 30rpx;
  border: 2rpx solid #eee;
}
.selected-images.data-v-fe65a6f0 {
  display: flex;
  flex-wrap: wrap;
  gap: 20rpx;
  margin-bottom: 20rpx;
}
.image-item.data-v-fe65a6f0 {
  position: relative;
  width: 200rpx;
  height: 200rpx;
}
.uploaded-image.data-v-fe65a6f0 {
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
}
.image-delete.data-v-fe65a6f0 {
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  width: 40rpx;
  height: 40rpx;
  background-color: #ff4757;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.add-image-btn.data-v-fe65a6f0 {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  height: 200rpx;
  border: 2rpx dashed #ccc;
  border-radius: 12rpx;
  background-color: #f8f8f8;
}
.add-text.data-v-fe65a6f0 {
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}
.count-text.data-v-fe65a6f0 {
  font-size: 20rpx;
  color: #ccc;
  margin-top: 5rpx;
}
.disclaimer.data-v-fe65a6f0 {
  margin-bottom: 40rpx;
}
.disclaimer-text.data-v-fe65a6f0 {
  font-size: 24rpx;
  color: #ff4757;
  line-height: 1.5;
}
.bottom-buttons.data-v-fe65a6f0 {
  margin-top: 40rpx;
  display: flex;
  gap: 20rpx;
}
.modify-btn.data-v-fe65a6f0,
.publish-btn.data-v-fe65a6f0 {
  flex: 1;
  height: 88rpx;
  border: none;
  border-radius: 44rpx;
  font-size: 32rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
  justify-content: center;
}
.modify-btn.data-v-fe65a6f0 {
  background-color: #f0f0f0;
  color: #666;
}
.publish-btn.data-v-fe65a6f0 {
  background-color: #ff4757;
  color: #fff;
}
.publish-btn.disabled.data-v-fe65a6f0 {
  background-color: #ccc;
  color: #999;
}
