}

/* 主要内容区域 */
.main-content.data-v-85330c9d {
  padding-top: 88rpx;
  padding: 88rpx 30rpx 30rpx;
}

/* 搜索区域 */
.search-section.data-v-85330c9d {
  display: flex;
  align-items: center;
  gap: 20rpx;
  margin-bottom: 40rpx;
}
.search-input-wrapper.data-v-85330c9d {
  flex: 1;
  display: flex;
  align-items: center;
  background-color: #fff;
  border-radius: 25rpx;
  padding: 20rpx 30rpx;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.1);
}
.search-input.data-v-85330c9d {
  flex: 1;
  margin-left: 20rpx;
  font-size: 28rpx;
  background: transparent;
  border: none;
}
.search-btn.data-v-85330c9d {
  background: linear-gradient(135deg, #28B389 0%, #20a085 100%);
  color: #fff;
  padding: 20rpx 30rpx;
  border-radius: 25rpx;
  font-size: 15rpx;
  font-weight: 400;
  border: none;
  box-shadow: 0 4rpx 15rpx rgba(40, 179, 137, 0.3);
  transition: all 0.3s ease;
  position: relative;
  overflow: hidden;
}
.search-btn.data-v-85330c9d:active {
  transform: translateY(2rpx);
  box-shadow: 0 2rpx 8rpx rgba(40, 179, 137, 0.4);
}
.search-btn.data-v-85330c9d::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255,255,255,0.2), transparent);
  transition: left 0.5s;
}
.search-btn.data-v-85330c9d:hover::before {
  left: 100%;
}

/* 结果区域 - 列表布局 */
.result-section.data-v-85330c9d {
