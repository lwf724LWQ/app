  flex-direction: column;
  align-items: center;
  gap: 12rpx;
  transition: all 0.2s ease;
  border: 1rpx solid #f0f0f0;
  text-align: center;
}
.result-card.data-v-85330c9d:active {
  transform: scale(0.99);
  box-shadow: 0 2rpx 12rpx rgba(0, 0, 0, 0.1);
}

