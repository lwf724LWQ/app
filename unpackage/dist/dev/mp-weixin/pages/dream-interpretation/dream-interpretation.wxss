  gap: 16rpx;
}
.result-card.data-v-85330c9d {
  background-color: #fff;
  border-radius: 12rpx;
  padding: 20rpx;
  box-shadow: 0 1rpx 8rpx rgba(0, 0, 0, 0.06);
